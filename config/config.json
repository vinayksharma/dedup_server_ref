  "database": {
    "retry": {
      "backoff_base_ms": 100,
      "max_attempts": 3,
      "max_backoff_ms": 1000
    },
    "timeout": {
      "busy_timeout_ms": 30000,
      "operation_timeout_ms": 60000
    }
  },
  "dedup_mode": "QUALITY",
  "duplicate_linker_check_interval": 10,
  "log_level": "DEBUG",
  "pre_process_quality_stack": true,
  "processing": {
    "batch_size": 200
  },
  "scan_interval_seconds": 300,
  "server_host": "localhost",
  "server_port": 8080,
  "threading": {
    "database_threads": 2,
    "max_processing_threads": 8,
    "max_scan_threads": 4
  },
  "video_processing": {
    "BALANCED": {
      "frames_per_skip": 2,
      "skip_count": 8,
      "skip_duration_seconds": 1
    },
    "FAST": {
      "frames_per_skip": 2,
      "skip_count": 5,
      "skip_duration_seconds": 2
    },
    "QUALITY": {
      "frames_per_skip": 3,
      "skip_count": 12,
      "skip_duration_seconds": 1
    }

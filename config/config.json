{
  "auth_secret": "test-secret-key",
  "cache": {
    "decoder_cache_size_mb": 512
  },
  "categories": {
    "audio": {
      "aac": true,
      "flac": true,
      "m4a": true,
      "mp3": true,
      "ogg": true,
      "wav": true
    },
    "images": {
      "gif": false,
      "jpeg": true,
      "jpg": true,
      "png": true
    },
    "video": {
      "avi": true,
      "mov": true,
      "mp4": true
    }
  },
  "database": {
    "retry": {
      "backoff_base_ms": 200,
      "max_attempts": 5,
      "max_backoff_ms": 2000
    },
    "timeout": {
      "busy_timeout_ms": 45000,
      "operation_timeout_ms": 90000
    }
  },
  "dedup_mode": "BALANCED",
  "log_level": "DEBUG",
  "pre_process_quality_stack": true,
  "processing": {
    "batch_size": 50
  },
  "processing_interval_seconds": 900,
  "scan_interval_seconds": 1800,
  "server_host": "test-host",
  "server_port": 9090,
  "threading": {
    "database_threads": 1,
    "max_decoder_threads": 2,
    "max_processing_threads": 4,
    "max_scan_threads": 2
  },
  "transcoding": {
    "cr2": true,
    "nef": false
  }
}